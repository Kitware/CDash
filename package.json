    "jquery": "^3.2",
    "laravel-mix": "^4.0.7",
    "lodash": "^4.17.5",
    "popper.js": "^1.12",
    "protractor": "^5.4.2",
    "resolve-url-loader": "^2.3.1",
    "sass": "^1.15.2",
